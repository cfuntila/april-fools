{

"manifest_version": 2,
"name": "April Fools Hacker",
"description": "Prank your friends by downloading this extension on their laptop and clicking it.",
"version": "0.1",
"web_accessible_resources": [
  "*.jpg"
],
"content_scripts": [
    {
    "matches": ["<all_urls>"],
    "js": ["content.js"]
    }
],

"background":{
    "scripts": ["background.js"]
},
"browser_action": {
    "default_icon": "fire.png"
    
}


}